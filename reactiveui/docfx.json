{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "/**/Akavache.csproj",
            "/**/Akavache.Core.csproj",
            "/**/Akavache.Drawing.csproj",
            "/**/Akavache.Mobile.csproj",
            "/**/Akavache.Sqlite3.csproj",
            "/**/Fusillade.csproj",
            "/**/Punchclock.-BuildIssue-csproj",
            "/**/ReactiveUI/bin/*/net472/ReactiveUI.dll",
            "/**/ReactiveUI.AndroidSupport.-BuildIssue-csproj",
            "/**/ReactiveUI.AndroidX/bin/*/monoandroid13.0/ReactiveUI.AndroidX.dll",
            "/**/ReactiveUI.Blazor/bin/*/netstandard2.0/ReactiveUI.Blazor.dll",
            "/**/ReactiveUI.Blend/bin/*/net472/ReactiveUI.Blend.dll",
            "/**/ReactiveUI.Drawing/bin/*/net472/ReactiveUI.Drawing.dll",
            "/**/ReactiveUI.Fody/bin/*/netstandard2.0/ReactiveUI.Fody.dll",
            "/**/ReactiveUI.Fody.Analyzer/bin/*/netstandard2.0/ReactiveUI.Fody.Analyzer.dll",
            "/**/ReactiveUI.Fody.Helpers/bin/*/netstandard2.0/ReactiveUI.Fody.Helpers.dll",
            "/**/ReactiveUI.Maui.csproj",
            "/**/ReactiveUI.Testing/bin/*/net472/ReactiveUI.Testing.dll",
            "/**/ReactiveUI.Winforms/bin/*/net472/ReactiveUI.Winforms.dll",
            "/**/ReactiveUI.WinUI/bin/*/net7.0-windows10.0.17763.0/ReactiveUI.WinUI.dll",
            "/**/ReactiveUI.Wpf/bin/*/net472/ReactiveUI.Wpf.dll",
            "/**/ReactiveUI.XamForms.csproj",
            "/**/ReactiveUI.Validation.-BuildIssue-csproj",
            "/**/ReactiveUI.Validation.AndroidSupport.-BuildIssue-csproj",
            "/**/ReactiveUI.Validation.AndroidX.-BuildIssue-csproj",
            "/**/Splat.csproj",
            "/**/Splat.AppCenter.csproj",
            "/**/Splat.ApplicationInsights.csproj",
            "/**/Splat.Autofac.csproj",
            "/**/Splat.Drawing.csproj",
            "/**/Splat.DryIoc.csproj",
            "/**/Splat.Exceptionless.csproj",
            "/**/Splat.Log4Net.csproj",
            "/**/Splat.Microsoft.Extensions.DependencyInjection.csproj",
            "/**/Splat.Microsoft.Extensions.Logging.csproj",
            "/**/Splat.Ninject.csproj",
            "/**/Splat.NLog.csproj",
            "/**/Splat.Prism.csproj",
            "/**/Splat.Raygun.csproj",
            "/**/Splat.Serilog.csproj",
            "/**/Splat.SimpleInjector.csproj",
            "/**/DynamicData.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net7.0"
      },
      "outputFormat": "mref",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "enumSortOrder": "alphabetic",
      "allowCompilationErrors": false,
      "filter": "filter.yml"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "An advanced, composable, reactive model-view-viewmodel framework",
      "_appLogoPath": "images/logoSmall.png",
      "_appFaviconPath": "images/favicons/favicon.ico",
      "_disableContribution": true,
      "_appFooter": "Copyright (c) .NET Foundation and Contributors",
      "_appName": "ReactiveUI"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "Announcements/**.md",
          "Announcements/**/toc.yml",
          "articles/**.md",
          "articles/**/toc.yml",
          "Book/**.md",
          "Book/**/toc.yml",
          "contribute/**.md",
          "contribute/**/toc.yml",
          "docs/**.md",
          "docs/**/toc.yml",
          "Extensions/**.md",
          "Extensions/**/toc.yml",
          "vs/**.md",
          "vs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}

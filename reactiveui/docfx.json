{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "/**/Akavache.csproj",
            "/**/Akavache.Core.csproj",
            "/**/Akavache.Drawing.csproj",
            "/**/Akavache.Mobile.csproj",
            "/**/Akavache.Sqlite3.csproj",
            "/**/Fusillade.csproj",
            "/**/Punchclock.-ReactiveIssue-csproj",
            "/**/ReactiveUI.csproj",
            "/**/ReactiveUI.AndroidSupport.-GlobalUsingIssue-csproj",
            "/**/ReactiveUI.AndroidX.-GlobalUsingIssue-csproj",
            "/**/ReactiveUI.Blazor.-GlobalUsingIssue-csproj",
            "/**/ReactiveUI.Blend.-GlobalUsingIssue-csproj",
            "/**/ReactiveUI.Drawing.-GlobalUsingIssue-csproj",
            "/**/ReactiveUI.Fody.csproj",
            "/**/ReactiveUI.Maui.csproj",
            "/**/ReactiveUI.Testing.csproj",
            "/**/ReactiveUI.Winforms.-GlobalUsingIssue-csproj",
            "/**/ReactiveUI.WinUI.-GlobalUsingIssue-csproj",
            "/**/ReactiveUI.Wpf.-GlobalUsingIssue-csproj",
            "/**/ReactiveUI.XamForms.csproj",
            "/**/Splat.csproj",
            "/**/Splat.AppCenter.csproj",
            "/**/Splat.ApplicationInsights.csproj",
            "/**/Splat.Autofac.csproj",
            "/**/Splat.Drawing.csproj",
            "/**/Splat.DryIoc.csproj",
            "/**/Splat.Exceptionless.csproj",
            "/**/Splat.Log4Net.csproj",
            "/**/Splat.Microsoft.Extensions.DependencyInjection.csproj",
            "/**/Splat.Microsoft.Extensions.Logging.csproj",
            "/**/Splat.Ninject.csproj",
            "/**/Splat.NLog.csproj",
            "/**/Splat.Prism.csproj",
            "/**/Splat.Raygun.csproj",
            "/**/Splat.Serilog.csproj",
            "/**/Splat.SimpleInjector.csproj",
            "/**/DynamicData.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net7.0"
      },
      "outputFormat": "mref",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "enumSortOrder": "alphabetic",
      "allowCompilationErrors": false,
      "filter": "filter.yml"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "An advanced, composable, reactive model-view-viewmodel framework",
      "_appLogoPath": "images/logoSmall.png",
      "_appFaviconPath": "images/favicons/favicon.ico",
      "_disableContribution": true,
      "_appFooter": "Copyright (c) .NET Foundation and Contributors",
      "_appName": "ReactiveUI"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "Announcements/**.md",
          "Announcements/**/toc.yml",
          "articles/**.md",
          "articles/**/toc.yml",
          "Book/**.md",
          "Book/**/toc.yml",
          "contribute/**.md",
          "contribute/**/toc.yml",
          "docs/**.md",
          "docs/**/toc.yml",
          "Extensions/**.md",
          "Extensions/**/toc.yml",
          "vs/**.md",
          "vs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
